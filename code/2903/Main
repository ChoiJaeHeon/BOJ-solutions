import java.util.Scanner;

public class Main {

    public static void main(String[] args) {

        Scanner sc = new Scanner(System.in);

        int rep = sc.nextInt();
        int init = 2;
        int ret = 0;

        for (int i = 0; i < rep; i++) {
            init = init + (init -1);
            ret = init * init;
        }

        System.out.print(ret);
    }
}
